#define VER_FILEVERSION             1,5,0,1500
#define VER_FILEVERSION_STR         "1.5.0.1500\0"

#define VER_PRODUCTVERSION          1,5,0,1500
#define VER_PRODUCTVERSION_STR      "1.5.0.1500\0"

1 VERSIONINFO
FILEVERSION     VER_FILEVERSION
PRODUCTVERSION  VER_PRODUCTVERSION
FILEOS          VOS__WINDOWS32
FILETYPE        VFT_APP
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "000004b0"
        BEGIN
            VALUE "CompanyName",      "Skymatic GmbH"
            VALUE "FileDescription",  "Cryptomator"
            VALUE "FileVersion",      VER_FILEVERSION_STR
			VALUE "Full Version",     VER_FILEVERSION_STR
            VALUE "InternalName",     "Cryptomator"
            VALUE "LegalCopyright",   "\xA9 2016 - 2020 Skymatic GmbH"
            VALUE "OriginalFilename", "Cryptomator.exe"
            VALUE "ProductName",      "Cryptomator"
            VALUE "ProductVersion",   VER_PRODUCTVERSION_STR
        END
    END

    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0000, 1200
    END
END